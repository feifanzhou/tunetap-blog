<%= render 'shared/search_bar' %>
<% if @is_logged_in %>
  <%= render 'shared/new_post' %>
<% end %>
<div id='posts'>
  <% @posts.each do |p| %>
    <% vote = (@votes.select { |vote| vote.post_id == p.id }).first %>
    <%= render_post_partial(p, vote, false, @is_logged_in) %>
  <% end %>
</div>
<% if @page_count > 1 %>
  <%= render 'shared/pager', page: @page, page_count: @page_count, path_base: @page_path_base %>
<% end %>
<% if @is_logged_in %>
  <%= render 'shared/admin_footer' %>
<% end %>